    "version": "0.2.0",
    "configurations": [
        {
            "name": "ACTL debug",
            "type": "python",
            "request": "launch",
            "program": "./library/actl/run.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "./library/"
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "./library/"
            }
        }
    ]
}